{
  "name": "aphp-formbuilder",
  "version": "1.8.0",
  "description": "Build LHC-Forms and FHIR Questionnaires",
  "homepage": "https://github.com/aphp/formbuilder.git",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/aphp/formbuilder.git"
  },
  "config": {
    "testPort": "9031",
    "devPort": "9030"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port ${npm_package_config_devPort} -c development",
    "start-public": "node ./bin/start-public.js",
    "install-and-build": "source ./bashrc.formbuilder && npm install && npm run clean-build",
    "clean-build": "source ./bashrc.formbuilder && ng cache clean && npm ci && source ./bashrc.formbuilder && npm run build",
    "copy-lforms": "rm -rf ./src/lib/lforms && node ./bin/copy-lforms.js",
    "build": "npm run copy-lforms && ng build --configuration production",
    "test": "source ./bashrc.formbuilder && npm run unit-test && npm run ci:cy-run",
    "unit-test": "ng test --watch=false --browsers ChromeHeadless",
    "lint": "ng lint",
    "cy:verify": "cypress verify",
    "cy:open": "cypress open --config baseUrl=http://localhost:${npm_package_config_testPort}",
    "cy:run": "cypress run --browser chrome --config baseUrl=http://localhost:${npm_package_config_testPort}",
    "ci:cy-run": "start-server-and-test ci:start-server http://localhost:${npm_package_config_testPort} cy:run",
    "ci:cy-open": "start-server-and-test ci:start-server http://localhost:${npm_package_config_testPort} cy:open",
    "ci:start-server": "angular-http-server --silent --path ./dist/aphp-formbuilder -p ${npm_package_config_testPort}",
    "e2e": "ng e2e",
    "e2e:prod": "source ./bashrc.formbuilder && ng run aphp-formbuilder:e2e:production"
  },
  "files": [
    "dist/aphp-formbuilder"
  ],
  "dependencies": {
    "@angular-builders/custom-webpack": "^17.0.2",
    "@angular/animations": "~17.3.9",
    "@angular/cdk": "^17.3.9",
    "@angular/common": "~17.3.9",
    "@angular/compiler": "~17.3.9",
    "@angular/core": "^17.3.9",
    "@angular/forms": "~17.3.9",
    "@angular/localize": "~17.3.9",
    "@angular/material": "^17.3.9",
    "@angular/material-moment-adapter": "^17.3.9",
    "@angular/platform-browser": "~17.3.9",
    "@angular/platform-browser-dynamic": "~17.3.9",
    "@angular/router": "~17.3.9",
    "@bugsplat/angular-tree-component": "^13.0.1",
    "@fortawesome/angular-fontawesome": "^0.14.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fullcalendar/core": "^6.1.5",
    "@lhncbc/ngx-schema-form": "2.11.0-forked2",
    "@lhncbc/ucum-lhc": "^5.0.3",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@types/fhir": "^0.0.36",
    "ace-builds": "^1.16.0",
    "bootstrap": "^5.2.3",
    "date-fns": "^2.30.0",
    "fast-copy": "^2.1.1",
    "fhirclient": "^2.5.2",
    "jexl": "^2.3.0",
    "jquery": "^3.6.3",
    "json-schema-traverse": "^1.0.0",
    "keycloak-angular": "^15.3.0",
    "keycloak-js": "^21.0.0",
    "lforms-loader": "^3.0.3",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "ngx-bootstrap": "^6.2.0",
    "ngx-bootstrap-multiselect": "^4.0.0",
    "papaparse": "^5.4.1",
    "rxjs": "~7.4.0",
    "traverse": "^0.6.6",
    "tslib": "^2.1.0",
    "uuid": "^9.0.1",
    "z-schema": "~4.2.2",
    "zone.js": "~0.14.5"
  },
  "devDependencies": {
    "@angular-eslint/builder": "17.4.1",
    "@angular-eslint/eslint-plugin": "17.4.1",
    "@angular-eslint/eslint-plugin-template": "17.4.1",
    "@angular-eslint/schematics": "17.4.1",
    "@angular-eslint/template-parser": "17.4.1",
    "@angular/cli": "^17.3.7",
    "@angular/compiler-cli": "~17.3.9",
    "@angular/language-service": "~17.3.9",
    "@cypress/schematic": "^2.5.1",
    "@types/hapi": "18.0.8",
    "@types/jasmine": "~4.3.1",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@webcomponents/webcomponentsjs": "^2.7.0",
    "angular-http-server": "^1.11.1",
    "cypress": "^13.6.4",
    "cypress-fail-on-console-error": "^4.0.2",
    "eslint": "^8.33.0",
    "jasmine-core": "^4.6.0",
    "jasmine-spec-reporter": "~7.0.0",
    "json-pointer": "^0.6.1",
    "jsonpointer": "^5.0.1",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "postcss": "^8.4.38",
    "start-server-and-test": "2.0.3",
    "ts-node": "~9.1.1",
    "typescript": "^5.4.5"
  }
}
